select top 10   
TBL.ID [ID],   
(
  STUFF(
  (
	Select  '-' + 	CAST(ISNULL(tbl1.TypeID,0) AS VARCHAR)				
	From tbl0(nolock) AUX left join tbl1 (nolock) On tbl1.ID = AUX.ID	 
	WHERE AUX.IdNum = TBL.IdNum  
	ORDER BY tbl1.TypeID ASC
	FOR XML PATH ('')
  ),1,1,'')
) [TYPES]
from tbl0 TBL
ORDER BY ID DESC



RES:

ID	TYPES
3533	0
355232	0
35231	0
35540	3-44
35529	0
35528	0
34227	46
4226	30
35425	7
35424	10
