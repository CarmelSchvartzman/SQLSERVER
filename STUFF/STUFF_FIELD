select top 10   TBL.ID [ID],   (
STUFF(
(
	Select  '-' + 	CAST(ISNULL(tbl1.TypeID,0) AS VARCHAR)				
	From tbl0(nolock) AUX left join tbl1 (nolock) On tbl1.ID = AUX.ID	 
	WHERE AUX.IdNum = TBL.IdNum  
	ORDER BY tbl1.TypeID ASC
	FOR XML PATH ('')
),1,1,'')
) [TYPES]
from tbl0 TBL
ORDER BY ID DESC



